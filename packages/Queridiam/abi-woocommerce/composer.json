{
    "name": "Queridiam/laravel-woocommerce",
    "description": "WooCommerce API Client for Laravel - aBillander",
    "homepage": "https://github.com/MQueridiam",
    "license": "MIT",
    "keywords": [
        "aBillander",
        "laravel",
        "wordpress",
        "woocommerce",
        "rest",
        "api"
    ],
    "authors": [
        {
            "name": "Markus Queridiam",
            "email": "wasp@abillander.com"
        }
    ],
    "require": {
        "php" : "^7.0|^8.0",
        "automattic/woocommerce": "1.3.*",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Queridiam\\WooCommerce\\": "src"
        }
    },
      "autoload-dev": {
        "psr-4": {
          "Queridiam\\WooCommerce\\Test\\": "tests"
        }
      },
      "require-dev": {
        "phpunit/phpunit": "^6.2",
        "mockery/mockery": "^0.9.4",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
      },
      "extra": {
        "laravel": {
          "providers": [
            "Queridiam\\WooCommerce\\ServiceProvider"
          ],
          "aliases": {
            "WooCommerce": "Queridiam\\WooCommerce\\Facades\\WooCommerce"
          }
        }
      },
    "minimum-stability": "dev"
}
